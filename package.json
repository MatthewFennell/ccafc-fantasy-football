  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "statements": 60,
        "branches": 60,
        "functions": 60,
        "lines": 60
      }
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/App.jsx",
      "!src/index.jsx",
      "!src/rootReducer.js",
      "!src/rootSaga.js",
      "!src/routes.jsx",
      "!src/serviceWorker.js",
      "!src/config/fbConfig.js",
      "!src/api/api.js",
      "!src/transfers/nextfixtures/helpers.jsx",
      "!src/notifications/saga.js",
      "!src/RenderRoutes.jsx",
      "!src/**/api.js"
    ]
  }
